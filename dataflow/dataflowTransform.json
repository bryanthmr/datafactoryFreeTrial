{
	"name": "dataflowTransform",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "RawData",
						"type": "DatasetReference"
					},
					"name": "RawData"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "CreateColumns",
					"description": "Creation d'une colonne Mois , et ordername\n"
				},
				{
					"name": "RemoveColumns",
					"description": "suppression de order date time , first name et last name"
				},
				{
					"name": "AggregateColumns"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "sort1"
				},
				{
					"name": "sort2"
				},
				{
					"name": "aggregate2"
				},
				{
					"name": "aggregate3"
				},
				{
					"name": "sort3"
				},
				{
					"name": "aggregate4"
				},
				{
					"name": "aggregate5"
				},
				{
					"name": "sort4"
				},
				{
					"name": "aggregate6"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id_orders as integer,",
				"          order_datetime as date,",
				"          id_product as integer,",
				"          quantity as integer,",
				"          product_name as string,",
				"          unit_price as float,",
				"          id_customer as integer,",
				"          {first name} as string,",
				"          {last name} as string,",
				"          age as integer,",
				"          groupe_client as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> RawData",
				"RawData derive(order_month = month(order_datetime),",
				"          order_name = concat({first name},' ',{last name}),",
				"          total_order = quantity * unit_price) ~> CreateColumns",
				"CreateColumns select(mapColumn(",
				"          id_orders,",
				"          order_month,",
				"          id_product,",
				"          quantity,",
				"          product_name,",
				"          unit_price,",
				"          id_customer,",
				"          order_name,",
				"          age,",
				"          groupe_client,",
				"          total_order",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RemoveColumns",
				"RemoveColumns aggregate(groupBy(order_month),",
				"     CA = sum(total_order),",
				"          nbOrders = count(id_orders),",
				"          quantitySold = sumIf(quantity >= 0,quantity),",
				"          quantityReturned = sumIf(quantity<0, quantity),",
				"          avgQuantity = avg(quantity),",
				"          nbCustomers = countDistinct(id_customer, order_name),",
				"          avgAge = avg(age),",
				"          avgPurchaseAmount = avg(total_order)) ~> AggregateColumns",
				"RemoveColumns aggregate(groupBy(order_month,",
				"          order_name),",
				"     CA = sum(total_order)) ~> aggregate1",
				"aggregate1 sort(asc(order_month, true),",
				"     desc(CA, true)) ~> sort1",
				"AggregateColumns sort(asc(order_month, true)) ~> sort2",
				"sort1 aggregate(groupBy(order_month),",
				"     bestCustomer = first(order_name)) ~> aggregate2",
				"RemoveColumns aggregate(groupBy(order_month,",
				"          product_name),",
				"     CA = sum(total_order)) ~> aggregate3",
				"aggregate3 sort(asc(order_month, true),",
				"     desc(CA, true)) ~> sort3",
				"sort3 aggregate(groupBy(order_month),",
				"     bestProduct = first(product_name)) ~> aggregate4",
				"RemoveColumns aggregate(groupBy(order_month,",
				"          groupe_client),",
				"     CA = sum(total_order)) ~> aggregate5",
				"aggregate5 sort(asc(order_month, true),",
				"     desc(CA, true)) ~> sort4",
				"sort4 aggregate(groupBy(order_month),",
				"     bestGroupeClient = first(groupe_client)) ~> aggregate6"
			]
		}
	}
}