{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CustomersCSV",
						"type": "DatasetReference"
					},
					"name": "Customers"
				},
				{
					"dataset": {
						"referenceName": "OrdersCSV",
						"type": "DatasetReference"
					},
					"name": "Orders"
				},
				{
					"dataset": {
						"referenceName": "ProductsCSV",
						"type": "DatasetReference"
					},
					"name": "Products"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "FilteredData",
						"type": "DatasetReference"
					},
					"name": "FilteredData"
				}
			],
			"transformations": [
				{
					"name": "OrdersProducts"
				},
				{
					"name": "columnRemoved",
					"description": "doublon product id removed"
				},
				{
					"name": "rawDataMerged"
				},
				{
					"name": "RemoveColumns",
					"description": "Id customers doublon removed"
				},
				{
					"name": "createKey"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id_customer as string,",
				"          {first name} as string,",
				"          {last name} as string,",
				"          age as string,",
				"          groupe_client as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Customers",
				"source(output(",
				"          id_customer as string,",
				"          order_datetime as string,",
				"          id_product as string,",
				"          quantity as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Orders",
				"source(output(",
				"          product_id as string,",
				"          product_name as string,",
				"          unit_price as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Products",
				"Orders, Products join(id_product == product_id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> OrdersProducts",
				"OrdersProducts select(mapColumn(",
				"          id_customer,",
				"          order_datetime,",
				"          id_product,",
				"          quantity,",
				"          product_name,",
				"          unit_price",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> columnRemoved",
				"columnRemoved, Customers join(columnRemoved@id_customer == Customers@id_customer,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> rawDataMerged",
				"rawDataMerged select(mapColumn(",
				"          order_datetime,",
				"          id_product,",
				"          quantity,",
				"          product_name,",
				"          unit_price,",
				"          id_customer,",
				"          {first name},",
				"          {last name},",
				"          age,",
				"          groupe_client",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RemoveColumns",
				"RemoveColumns keyGenerate(output(id_orders as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> createKey",
				"createKey sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          id_orders as string,",
				"          order_datetime as string,",
				"          id_product as string,",
				"          quantity as string,",
				"          product_name as string,",
				"          unit_price as string,",
				"          id_customer as string,",
				"          {first name} as string,",
				"          {last name} as string,",
				"          age as string,",
				"          groupe_client as string",
				"     ),",
				"     partitionFileNames:['FilteredData.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     partitionBy('hash', 1)) ~> FilteredData"
			]
		}
	}
}